{
  "name": "typescript-jira",
  "version": "1.0.0",
  "description": "read Jira Json files and create SQL ready data",
  "scripts": {
    "dev": "concurrently \"npm:build:dev\" \"npm:start:dev\"",
    "build:dev": "tsc --watch --preserveWatchOutput",
    "start:dev": "nodemon dist/index.js",
    "test": "jest"
  },
  "keywords": [
    "typescript",
    "Jira",
    "Json",
    "Azure",
    "SQL"
  ],
  "author": "Robert PASTOR",
  "license": "ISC",
  "devDependencies": {
    "@types/event-stream": "^3.3.34",
    "@types/stack-trace": "0.0.29",
    "concurrently": "^6.1.0",
    "jest": "^26.6.3",
    "nodemon": "^2.0.7",
    "typescript": "^4.2.4"
  },
  "dependencies": {
    "@types/tedious": "^4.0.3",
    "event-stream": "^4.0.1",
    "stack-trace": "0.0.10",
    "tedious": "^11.0.8"
  }
}
